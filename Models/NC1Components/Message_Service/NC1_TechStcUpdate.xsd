  <xs:element name="NC1_TechStcUpdate">
    <xs:annotation>
      <xs:documentation>Le message NC1_TechStcUpdate est utilisé pour mettre à jour la Situation Tactique Courante (STC). Il indique les objets NC1 à mettre à jour à retirer de la STC.</xs:documentation>
      <xs:documentation xml:lang="en">The NC1_TechStcUpdate message is used to update the Current Tactical Situation ( STC). He indicates objects NC1 to be updated to remove from the STC.</xs:documentation>
      <xs:appinfo>
        <nc1:type xmlns:nc1="urn:fra:nc1">Message de service</nc1:type>
        <nc1ihm:Nom>Mise à jour de la Situation Tactique Courante</nc1ihm:Nom>
        <nc1ihm:Nom xml:lang="en">Update of the tactical situation</nc1ihm:Nom>
        <nc1:MessH xmlns:nc1="urn:fra:nc1">N/A</nc1:MessH>
        <nc1:MessADatP3 xmlns:nc1="urn:fra:nc1">N/A</nc1:MessADatP3>
        <nc1:LastNC1ModifyVersion xmlns:nc1="urn:fra:nc1">9.0.0</nc1:LastNC1ModifyVersion>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="nc1msgcom:BaseMessageType">
          <xs:sequence>
            <xs:element name="isFull" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Indique si la situation tactique courante transmise par le message est complète (=1) ou partielle (=0).</xs:documentation>
                <xs:documentation xml:lang="en">Specifies if the tactical situation transmitted by the message is comprehensive (=1) or partial (=0).</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Mode de mise à jour</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Update mode</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="updatedObjects">
              <xs:annotation>
                <xs:documentation>Objets mis à jour de la Situation Tactique Courante.</xs:documentation>
                <xs:documentation xml:lang="en">Tactical situation updated objects.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Objets mis à jour</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Updated objects</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="object" type="nc1objcom:BaseObjectType">
                    <xs:annotation>
                      <xs:documentation>Objet mis à jour de la Situation Tactique Courante.</xs:documentation>
                      <xs:documentation xml:lang="en">Tactical situation updated object.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Objet mis à jour</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">Updated object</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="desactivatedObjects">
              <xs:annotation>
                <xs:documentation>Objets désactivés de la Situation Tactique Courante.</xs:documentation>
                <xs:documentation xml:lang="en">Tactical situation deactivated objects.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Objets désactivés</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Deactivated objects</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="object" type="nc1objcom:BaseObjectType">
                    <xs:annotation>
                      <xs:documentation>Objet désactivé de la Situation Tactique Courante.</xs:documentation>
                      <xs:documentation xml:lang="en">Deactivated object from the tactical situation.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Objet désactivé</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">deactivated object</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="nc1stcupdate:InformationTypeCode">
              <xs:annotation>
                <xs:documentation>Type d'information.</xs:documentation>
                <xs:documentation xml:lang="en">information type.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Type</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">information type</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
