  <xs:import namespace="urn:fra:nc1:objet:intelrequirement" schemaLocation="../Objet/NC1_IntelRequirement.xsd" />
  <xs:import namespace="urn:fra:nc1:objet:meteo" schemaLocation="../Objet/NC1_Meteo.xsd" />
  <xs:import namespace="urn:fra:nc1:objet:mission" schemaLocation="../Objet/NC1_Mission.xsd" />
  <xs:import namespace="urn:fra:nc1:objet:missionASA" schemaLocation="../Objet/NC1_MissionASA.xsd" />
  <xs:import namespace="urn:fra:nc1:objet:missionASS" schemaLocation="../Objet/NC1_MissionASS.xsd" />
  <xs:import namespace="urn:fra:nc1:objet:obstacle" schemaLocation="../Objet/NC1_Obstacle.xsd" />
