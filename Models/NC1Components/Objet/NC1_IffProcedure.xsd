      <xs:documentation xml:lang="en">IFF/SIF code.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-7][0-3][0-7]{4}" />
    </xs:restriction>
  </xs:simpleType>
