        <xs:element name="iffMode" type="nc1iffprocedure:IffModeType">
          <xs:annotation>
            <xs:documentation>Mode IFF.</xs:documentation>
            <xs:documentation xml:lang="en">IFF mode.</xs:documentation>
            <xs:appinfo>
              <nc1ihm:Nom>Mode IFF</nc1ihm:Nom>
