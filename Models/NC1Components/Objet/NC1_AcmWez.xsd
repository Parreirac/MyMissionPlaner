                      <xs:documentation xml:lang="en">ACM designator.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Désignateur</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">Designator</nc1ihm:Nom>
