                    <xs:annotation>
                      <xs:documentation>Nom de la zone aérienne.</xs:documentation>
                      <xs:documentation xml:lang="en">ACM name.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Nom</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">name</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
