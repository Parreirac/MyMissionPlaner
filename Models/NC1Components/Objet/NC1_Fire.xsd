                    <xs:documentation>Acteurs du tir.</xs:documentation>
                    <xs:documentation xml:lang="en">Fire actors.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Acteurs</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">Actors</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="adjustmentFire" type="nc1fire:AdjustmentFireType">
                  <xs:annotation>
                    <xs:documentation>Tir de mise en place.</xs:documentation>
                    <xs:documentation xml:lang="en">Adjustment fire.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>MEP</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">MEP</nc1ihm:Nom>
                    </xs:appinfo>
