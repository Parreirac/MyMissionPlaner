              <xs:sequence>
                <xs:element minOccurs="0" name="status" type="nc1sicsdicos:L114_36Code">
                  <xs:annotation>
                    <xs:documentation>Etat du tir (ex : « COUPPARTI »).</xs:documentation>
                    <xs:documentation xml:lang="en">"Status of the fire (e.g. ""COUPPARTI"")".</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Etat du tir</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">Status of the fire</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="name" type="nc1common:ShortTextObjectType">
                  <xs:annotation>
                    <xs:documentation>Nom libre pour les opérationnels.</xs:documentation>
                    <xs:documentation xml:lang="en">Name of the fire to be displayed on the map.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Nom</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">name</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
                <xs:element name="tno" type="nc1common:TnoType">
                  <xs:annotation>
                    <xs:documentation>Numéro de tir (TNO).</xs:documentation>
                    <xs:documentation xml:lang="en">TNO.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Numéro de tir</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">TNO</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="url" type="nc1common:MediumTextObjectType">
                  <xs:annotation>
                    <xs:documentation>Lien vers un fichier associé à l’objet.</xs:documentation>
                    <xs:documentation xml:lang="en">Link to a file associated with the object.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Url</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">url</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="comment" type="nc1common:LongTextObjectType">
                  <xs:annotation>
                    <xs:documentation>Commentaire en texte libre.</xs:documentation>
                    <xs:documentation xml:lang="en">free text comment.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Commentaire</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">comment</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="callForFireData">
            <xs:annotation>
              <xs:documentation>Informations relatives à la demande de tir.</xs:documentation>
              <xs:documentation xml:lang="en">Call for fire data.</xs:documentation>
              <xs:appinfo>
                <nc1ihm:Nom>Informations de demande de tir</nc1ihm:Nom>
                <nc1ihm:Nom xml:lang="en">Call for fire data</nc1ihm:Nom>
              </xs:appinfo>
            </xs:annotation>
