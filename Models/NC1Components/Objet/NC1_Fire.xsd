                    <xs:documentation xml:lang="en">Fire trajectory type.</xs:documentation>
                    <xs:appinfo>
                      <nc1ihm:Nom>Type de trajectoire</nc1ihm:Nom>
                      <nc1ihm:Nom xml:lang="en">Trajectory type</nc1ihm:Nom>
                    </xs:appinfo>
                  </xs:annotation>
                </xs:element>
