  <xs:element name="NC1_CCA-CARD">
    <xs:annotation>
      <xs:documentation>Le message NC1_CCA-CARD est utilisé pour diffuser une demande d'appui feu hélicoptère.</xs:documentation>
      <xs:documentation xml:lang="en">The NC1_CCA-CARD message is used to transmit a helicopter fire support request.</xs:documentation>
      <xs:appinfo>
        <nc1:type xmlns:nc1="urn:fra:nc1">Message</nc1:type>
        <nc1ihm:Nom>Demande d'appui feu hélicoptère</nc1ihm:Nom>
        <nc1ihm:Nom xml:lang="en">Helicopter fire support request</nc1ihm:Nom>
        <nc1:MessH xmlns:nc1="urn:fra:nc1">N/A</nc1:MessH>
        <nc1:MessADatP3 xmlns:nc1="urn:fra:nc1">N/A</nc1:MessADatP3>
        <nc1:LastNC1ModifyVersion xmlns:nc1="urn:fra:nc1">9.0.0</nc1:LastNC1ModifyVersion>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="nc1msgcom:BaseMessageType">
          <xs:sequence>
            <xs:element name="observer" type="nc1objcom:AgentType">
              <xs:annotation>
                <xs:documentation>LIGNES 1 et 2 : Observateur et localisation de l'observateur.</xs:documentation>
                <xs:documentation xml:lang="en">LINES 1 and 2: Observer and location of the observer.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Observateur et localisation</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Subject</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="target" type="nc1objcom:LocalisedObjectType">
              <xs:annotation>
                <xs:documentation>LIGNES 3 et 4 : Cible et localisation de la cible.</xs:documentation>
                <xs:documentation xml:lang="en">Lines 3 and 4: Target and location of the target.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Cible et localisation</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Target and location</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="additionalInformation" type="nc1msgcom:MixedAnyType">
              <xs:annotation>
                <xs:documentation>LIGNE 5 : Informations complémentaires.</xs:documentation>
                <xs:documentation xml:lang="en">LINE 5: Additional information.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Informations complémentaires</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Additional information</nc1ihm:Nom>
