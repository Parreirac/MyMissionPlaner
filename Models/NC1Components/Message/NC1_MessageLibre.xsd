  <xs:element name="NC1_MessageLibre">
    <xs:annotation>
      <xs:documentation>Le message NC1_MessageLibre est utilisé pour transmettre du texte libre ainsi que des objets encapsulés.</xs:documentation>
      <xs:documentation xml:lang="en">The NC1_MessageLibre message is used to transmit of the free text as well as the packaged objects.</xs:documentation>
      <xs:appinfo>
        <nc1:type xmlns:nc1="urn:fra:nc1">Message</nc1:type>
        <nc1ihm:Nom>Message libre</nc1ihm:Nom>
        <nc1ihm:Nom xml:lang="en">Free text message</nc1ihm:Nom>
        <nc1:MessH xmlns:nc1="urn:fra:nc1">N/A</nc1:MessH>
        <nc1:MessADatP3 xmlns:nc1="urn:fra:nc1">N/A</nc1:MessADatP3>
        <nc1:LastNC1ModifyVersion xmlns:nc1="urn:fra:nc1">9.0.0</nc1:LastNC1ModifyVersion>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="nc1msgcom:BaseMessageType">
          <xs:sequence>
            <xs:element minOccurs="0" name="objectsGlobal">
              <xs:annotation>
                <xs:documentation>Liste d'objets globale.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Liste d'objets globale</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="object" type="nc1objcom:BaseObjectType">
                    <xs:annotation>
                      <xs:documentation>Objet NC1.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Objet NC1</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="type" type="nc1common:ShortTextType">
              <xs:annotation>
                <xs:documentation>Type du message.</xs:documentation>
                <xs:documentation xml:lang="en">Message type.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Type</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Type</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="nature" type="nc1common:ShortTextType">
