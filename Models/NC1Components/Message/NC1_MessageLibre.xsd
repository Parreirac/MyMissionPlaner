                      <xs:documentation>Contenu de la partie du message.</xs:documentation>
                      <xs:documentation xml:lang="en">Message part content.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Contenu</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">content</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
