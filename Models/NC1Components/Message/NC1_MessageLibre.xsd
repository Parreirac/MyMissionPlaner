                <xs:documentation>Métier.</xs:documentation>
                <xs:documentation xml:lang="en">Profession.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Métier</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Profession</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="subject" type="nc1common:MediumTextType">
              <xs:annotation>
                <xs:documentation>Sujet du message.</xs:documentation>
                <xs:documentation xml:lang="en">Message subject.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Sujet</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Subject</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="isTemplate" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Indique si le message est un modèle.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Modèle</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" name="body">
              <xs:annotation>
                <xs:documentation>Corps du message.</xs:documentation>
                <xs:documentation xml:lang="en">Message body.</xs:documentation>
                <xs:appinfo>
                  <nc1ihm:Nom>Corps</nc1ihm:Nom>
                  <nc1ihm:Nom xml:lang="en">Body</nc1ihm:Nom>
                </xs:appinfo>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="title" type="nc1common:MediumTextType">
                    <xs:annotation>
                      <xs:documentation>Titre de la partie du message.</xs:documentation>
                      <xs:documentation xml:lang="en">Message part title.</xs:documentation>
                      <xs:appinfo>
                        <nc1ihm:Nom>Titre</nc1ihm:Nom>
                        <nc1ihm:Nom xml:lang="en">title</nc1ihm:Nom>
                      </xs:appinfo>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="hierarchyLevel" type="nc1msgcom:HierarchyLevelType">
                    <xs:annotation>
                      <xs:documentation>Niveau hiérarchique.</xs:documentation>
                      <xs:documentation xml:lang="en">Hierarchy level.</xs:documentation>
                      <xs:appinfo>
