        <nc1:type xmlns:nc1="urn:fra:nc1">Message</nc1:type>
        <nc1ihm:Nom>Annexe administration/logistique</nc1ihm:Nom>
        <nc1ihm:Nom xml:lang="en">Administration and logistic support annex</nc1ihm:Nom>
